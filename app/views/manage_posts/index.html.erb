<p id="notice"><%= notice %></p>

<h1>Manage Posts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manage_posts.each do |manage_post| %>
      <tr>
        <td><%= link_to 'Show', manage_post %></td>
        <td><%= link_to 'Edit', edit_manage_post_path(manage_post) %></td>
        <td><%= link_to 'Destroy', manage_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manage Post', new_manage_post_path %>
